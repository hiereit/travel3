<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00FFFFFF">

    <ImageView
        android:id="@+id/y_course_bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/y_course" />

    <TextView
        android:id="@+id/y_course_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:layout_marginLeft="9dp"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/tmoney_bold"
        android:text="n"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="@+id/y_course_bg"
        app:layout_constraintTop_toTopOf="@+id/y_course_bg" />

    <TextView
        android:id="@+id/y_course_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="23dp"
        android:layout_marginTop="35dp"
        android:fontFamily="@font/tmoney_bold"
        android:text="장소 이름"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="@+id/y_course_bg"
        app:layout_constraintTop_toTopOf="@+id/y_course_bg" />
</androidx.constraintlayout.widget.ConstraintLayout>